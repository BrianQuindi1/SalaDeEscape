@{
    ViewData["Title"] = "Victoria";
}

<html>
    <head>
        <title>Victoria</title>
    </head>
    <body class="fondoJugar">
        <div class="contenedorGeneral">
            <h1 class="textoTitulo">Victoria (8:01 am)</h1>
            <p class="texto">
